/*  弹出层 */
.popup {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 99;
}
.popup.show {
    display: block;
}
.popup.show .mask {
      -webkit-animation: showPopup 0.2s linear both;
              animation: showPopup 0.2s linear both;
}
.popup.show .layer {
      -webkit-animation: showLayer 0.2s linear both;
              animation: showLayer 0.2s linear both;
}
.popup.hide .mask {
    -webkit-animation: hidePopup 0.2s linear both;
            animation: hidePopup 0.2s linear both;
}
.popup.hide .layer {
    -webkit-animation: hideLayer 0.2s linear both;
            animation: hideLayer 0.2s linear both;
}
.popup.none {
    display: none;
}
.popup .mask {
    position: fixed;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
    background-color: rgba(0, 0, 0, 0.4);
}
.popup .layer {
    position: fixed;
    z-index: 99;
    bottom: 0;
    width: 100%;
    /* min-height: 40vh; */
    max-height: 60vh;
    overflow-y: auto;
    -webkit-border-radius: 10rpx 10rpx 0 0;
            border-radius: 10rpx 10rpx 0 0;
    background-color: #fff;
    padding: 30rpx;
}
.popup .layer .attr-list {
      width: 90%;
}
.popup .layer .attr-list .item-list {
        width: 100%;
        height: 88rpx;
        line-height: 88rpx;
        font-size: 30rpx;
        border-bottom: 1px solid #eee;
        color: #333333;
}
.popup .layer .classification {
      width: 180rpx;
      height: 80rpx;
      margin: auto;
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      margin-top: 40rpx;
      -webkit-box-orient: horizontal;
      -webkit-box-direction: normal;
      -webkit-flex-direction: row;
          -ms-flex-direction: row;
              flex-direction: row;
}
.popup .layer .classification .new-classification {
        width: 40rpx;
        height: 40rpx;
        background-size: cover;
}
.popup .layer .classification .class {
        font-size: 30rpx;
        margin-left: 20rpx;
        color: #C5091D;
}
.popup .class-layer {
    position: fixed;
    z-index: 99;
    width: 80%;
    height: 300rpx;
    top: 35%;
    left: 50%;
    -webkit-border-radius: 10px;
            border-radius: 10px;
    margin: auto;
    -ms-transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    margin: auto;
    max-height: 60vh;
    overflow-y: auto;
    -webkit-border-radius: 10rpx;
            border-radius: 10rpx;
    background-color: #fff;
}
.popup .class-layer .new-build {
      width: 100%;
      display: inline-block;
      height: 40rpx;
      line-height: 40rpx;
      font-size: 32rpx;
      color: #333;
      margin-top: 30rpx;
      text-align: center;
}
.popup .class-layer .new-input {
      width: 90%;
      margin-left: 5%;
      margin-top: 30rpx;
      display: inline-block;
      height: 80rpx;
      line-height: 80rpx;
      text-indent: 40rpx;
      font-size: 28rpx;
      color: #333;
      border: 1px solid #ececec;
      text-align: left;
}
.popup .class-layer .new-btn {
      margin-top: 10rpx;
      border-top: 1px solid #ececec;
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-box-orient: horizontal;
      -webkit-box-direction: normal;
      -webkit-flex-direction: row;
          -ms-flex-direction: row;
              flex-direction: row;
}
.popup .class-layer .new-btn .cancel, .popup .class-layer .new-btn .confirm {
        -webkit-box-flex: 1;
        -webkit-flex: 1;
            -ms-flex: 1;
                flex: 1;
        height: 100rpx;
        line-height: 100rpx;
        text-align: center;
        font-size: 32rpx;
        color: #333;
        border-right: 1px solid #ececec;
}
@-webkit-keyframes showPopup {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
@keyframes showPopup {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
@-webkit-keyframes hidePopup {
0% {
    opacity: 1;
}
100% {
    opacity: 0;
}
}
@keyframes hidePopup {
0% {
    opacity: 1;
}
100% {
    opacity: 0;
}
}
@-webkit-keyframes showLayer {
0% {
    -webkit-transform: translateY(120%);
            transform: translateY(120%);
}
100% {
    -webkit-transform: translateY(0%);
            transform: translateY(0%);
}
}
@keyframes showLayer {
0% {
    -webkit-transform: translateY(120%);
            transform: translateY(120%);
}
100% {
    -webkit-transform: translateY(0%);
            transform: translateY(0%);
}
}
@-webkit-keyframes hideLayer {
0% {
    -webkit-transform: translateY(0);
            transform: translateY(0);
}
100% {
    -webkit-transform: translateY(120%);
            transform: translateY(120%);
}
}
@keyframes hideLayer {
0% {
    -webkit-transform: translateY(0);
            transform: translateY(0);
}
100% {
    -webkit-transform: translateY(120%);
            transform: translateY(120%);
}
}
