<view class="content"><uni-nav-bar vue-id="1" left-icon="back" data-event-opts="{{[['^clickLeft',[['backButton']]]]}}" bind:clickLeft="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="navigation"><text data-event-opts="{{[['tap',[['singleRule',['$event']]]]]}}" class="left" catchtap="__e">单规格</text><text data-event-opts="{{[['tap',[['multipleRule',['$event']]]]]}}" class="right" catchtap="__e">多规格</text></view></uni-nav-bar><swiper class="swiper-box" catchtouchmove="stopTouchMove"><swiper-item class="tab-content"><scroll-view class="list-scroll-content swiper-no-swiping" scroll-y><block wx:if="{{orderList.length===0}}"><empty vue-id="2" bind:__l="__l"></empty></block><view class="add-goods"><view class="goods-list-item"><view class="goods-item"><text class="goods-title">商品名称</text><input class="goods-name" type="text" placeholder="请输入商品名称" data-event-opts="{{[['input',[['__set_model',['','goodsName','$event',[]]]]]]}}" value="{{goodsName}}" bindinput="__e"/></view><view class="goods-item"><text class="goods-title">商品类别</text><input class="goods-name" type="text" placeholder="请选择商品类别" disabled data-event-opts="{{[['tap',[['freightTemplate',[1]]]],['input',[['__set_model',['','classifyName','$event',[]]]]]]}}" value="{{classifyName}}" catchtap="__e" bindinput="__e"/><image class="sort-icon" src="../../static/sort-icon.png" mode></image></view><view class="goods-item-picture"><text class="goods-title">商品图片</text><view data-event-opts="{{[['tap',[['changeImg',['$event']]]]]}}" hidden="{{!(showImg)}}" class="add-pictrue" bindtap="__e"></view><image hidden="{{!(!showImg)}}" class="upload-img" src="{{uploadPictures}}" mode="aspectFill" data-event-opts="{{[['tap',[['lookBigImg',['$0'],['uploadPictures']]]]]}}" bindtap="__e"></image><image hidden="{{!(!showImg)}}" class="sort-icon" src="../../static/delete.png" data-event-opts="{{[['tap',[['deleteTemplate',['$event']]]]]}}" catchtap="__e"></image></view></view><view class="blank"></view><view class="goods-list-item "><view class="goods-item"><text class="goods-title">商品价格</text><input class="goods-name" type="number" placeholder="请输入商品价格" data-event-opts="{{[['input',[['__set_model',['','goodsPrice','$event',[]]]]]]}}" value="{{goodsPrice}}" bindinput="__e"/></view><view class="goods-item"><text class="goods-title">当前库存数量</text><input class="goods-name" type="number" placeholder="请输入当前库存数量" data-event-opts="{{[['input',[['__set_model',['','stockNum','$event',[]]]]]]}}" value="{{stockNum}}" bindinput="__e"/></view><view class="goods-item"><text class="goods-title">商品重量（KG）</text><input class="goods-name" type="text" placeholder="请输入商品重量" data-event-opts="{{[['input',[['__set_model',['','goodsWeight','$event',[]]]]]]}}" value="{{goodsWeight}}" bindinput="__e"/></view></view><view class="blank"></view><view class="goods-list-item"><view class="goods-item-detail"><text class="goods-title">详情描述</text><textarea placeholder="请输入详情描述" data-event-opts="{{[['input',[['__set_model',['','content','$event',[]]]]]]}}" value="{{content}}" bindinput="__e"></textarea></view><view class="goods-item"><text class="goods-title">预设开售时间</text><rui-date-picker class="goods-name" vue-id="3" fields="second" start="2010-00-00 00:00:00" end="2030-12-30 23:59:59" value="{{openTime}}" data-event-opts="{{[['^change',[['bindChange']]],['^cancel',[['bindCancel']]]]}}" bind:change="__e" bind:cancel="__e" bind:__l="__l"></rui-date-picker><image class="sort-icon" src="../../static/sort-icon.png" mode></image></view><view class="goods-item"><text class="goods-title">分销金额</text><input class="goods-name" type="number" placeholder="请输入分销金额" data-event-opts="{{[['input',[['__set_model',['','fenxiao','$event',[]]]]]]}}" value="{{fenxiao}}" bindinput="__e"/></view><view class="goods-item"><text class="goods-title">运费模版ID</text><input class="goods-name" type="text" placeholder="请选择" disabled data-event-opts="{{[['tap',[['freightTemplate',[2]]]],['input',[['__set_model',['','deliveryName','$event',[]]]]]]}}" value="{{deliveryName}}" catchtap="__e" bindinput="__e"/><image class="sort-icon" src="../../static/sort-icon.png" mode></image></view></view><view class="tab-list"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectItem',['$0'],[[['tabList','',index]]]]]]]}}" class="{{['tab-item',(goodsStatusIndex==index)?'active':'']}}" bindtap="__e">{{''+item.name+''}}</view></block></view><view class="list-blank"></view></view></scroll-view></swiper-item></swiper><block wx:if="{{showBigImg}}"><view data-event-opts="{{[['tap',[['cancelDownload',['$event']]]]]}}" class="modal-frame" bindtap="__e"><view class="modal-box"><image class="sort-icon" src="{{bigImg}}" mode></image></view></view></block><view class="tab-bar"><view data-event-opts="{{[['tap',[['addConfirm',['$event']]]]]}}" class="tab-item" bindtap="__e">确认添加</view></view><view data-event-opts="{{[['tap',[['toggleSpec',['$event']]]]]}}" class="{{['popup',specClass]}}" bindtap="__e"><view class="mask"></view><view class="layer"><block wx:for="{{freightList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="attr-list"><view data-event-opts="{{[['tap',[['toggleSpecTab',['$0'],[[['freightList','',index]]]]]]]}}" class="item-list" bindtap="__e">{{''+item.name+''}}</view></view></block></view></view></view>