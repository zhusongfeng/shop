<view class="container" bind:__l="__l"><block wx:if="{{address.phone!=undefined}}"><view class="center"><view class="name-tel"><view class="name">{{address.name}}</view><view class="tel">{{address.phone}}</view></view><view class="address">{{''+address.Area.province+" "+address.Area.city+address.Area.region+address.detail+''}}</view><view data-event-opts="{{[['tap',[['changeAddress',['$event']]]]]}}" class="change-address" bindtap="__e">更改地址</view></view></block><block wx:else><view class="center no-address"><view data-event-opts="{{[['tap',[['changeAddress',['$event']]]]]}}" class="change-address" bindtap="__e">请选择收货地址</view></view></block><view class="cart-list"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="index" wx:key="goods_id"><view id="{{item.goods_id}}" data-event-opts="{{[['tap',[['commodityDetails',['$0'],[[['cartList','goods_id',item.goods_id,'goods_id']]]]]]]}}" bindtap="__e"><view class="cart-item b-b"><view class="image-wrapper"><image class="{{[item.loaded]}}" src="{{item.image}}" mode="aspectFill" lazy-load data-event-opts="{{[['load',[['onImageLoad',['cartList',index]]]],['error',[['onImageError',['cartList',index]]]]]}}" bindload="__e" binderror="__e"></image></view><view class="item-right"><text class="clamp title">{{item.goods_name}}</text><text class="attr">{{(item.goods_sku.goods_attr?item.goods_sku.goods_attr:'')+" 数量："+item.total_num}}</text><text class="price">{{"¥"+item.goods_price}}</text></view></view></view></block></view><view class="total-commodity"><view class="total-box"><text class="total-name">商品数总计:</text><text class="total-number">{{quantityNumber}}</text></view><view class="total-box"><text class="total-name">商品价格总计：</text><text class="total-number">{{commodityPrice}}</text></view><view class="total-box"><text class="total-name">快递费用：</text><text class="total-number">{{freight}}</text></view></view><view class="action-section"><view class="total-box"><text class="price">{{"¥"+orderTotalPrice}}</text><block wx:if="{{freight}}"><text class="coupon">运费：<text>{{freight}}</text>元</text></block></view><button class="no-border confirm-btn" type="primary" data-event-opts="{{[['tap',[['createOrder',['$event']]]]]}}" bindtap="__e">去结算</button></view></view>