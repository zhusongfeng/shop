<view class="container" bind:__l="__l"><view class="score" style="border-top:1rpx solid #dddddd;"><text class="score clamp2">今日收入</text><text class="score clamp2">本月收入</text><text class="score clamp2">今年总收入</text></view><view class="score" style="border-bottom:1rpx solid #dddddd;"><text class="score clamp2" style="color:#fa436a;">{{day}}</text><text class="score clamp2" style="color:#fa436a;">{{month}}</text><text class="score clamp2" style="color:#fa436a;">{{year}}</text></view><scroll-view class="list-scroll-content" scroll-y="true" data-event-opts="{{[['scrolltolower',[['loadMore',['$event']]]]]}}" bindscrolltolower="__e"><view class="list"><block wx:if="{{loaded===true&&scoreList.length===0}}"><empty></empty></block><view class="collect-section"><block wx:for="{{scoreList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="collect-item"><view class="title"><text class="title clamp">{{item.memo}}</text><text class="title time">{{item.createtime}}</text></view><text class="price">{{"金额：￥"+item.money}}</text></view></block></view><block wx:if="{{scoreList!==0}}"><uni-load-more status="{{loadingType}}"></uni-load-more></block></view></scroll-view></view>